<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ absolute_url(path('front_home')) }}</loc>
    </url>
    {% for url in urls %}
        <url>
            <loc>{{ url.loc }}</loc>
            {% if url.lastmod is defined %}
                <lastmod>{{ url.lastmod|date('Y-m-d') }}</lastmod>
            {% endif %}
        </url>
    {% endfor %}
</urlset>
